{
  "window.title": "${activeEditorLong}${separator}${rootName}",
  "editor.lineNumbers": "relative",
  "editor.fontSize": 20,
  "editor.fontFamily": "Jetbrains Mono,OperatorMono Nerd Font, Menlo, Monaco, 'Courier New', monospace",
  "editor.minimap.enabled": false,
  "vim.leader": ";",
  "vim.useCtrlKeys": false,
  "vim.easymotion": true,
  "vim.otherModesKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "f",
      ],
      "after": [
        "<leader>",
        "<leader>",
        "f"
      ],
    },
    {
      "before": [
        "<leader>",
        "F",
      ],
      "after": [
        "<leader>",
        "<leader>",
        "F"
      ],
    },
    {
      "before": [
        "u",
      ],
      "commands": [
        {
          "command": "undo"
        }
      ]
    },
    {
      "before": [
        "g",
        "w",
        "h"
      ],
      "commands": [
        {
          "command": "workbench.action.focusPreviousGroup"
        }
      ]
    },
    {
      "before": [
        "g",
        "w",
        "l"
      ],
      "commands": [
        {
          "command": "workbench.action.focusNextGroup"
        }
      ]
    },
    {
      "before": [
        "c",
        "o",
        "w"
      ],
      "commands": [
        {
          "command": "editor.action.toggleWordWrap"
        }
      ]
    },
    {
      "before": [
        "c",
        "o",
        "k"
      ],
      "after": [],
      "commands": [
        {
          "command": "settings.cycle.colorTheme",
        }
      ]
    },
    {
      "before": [
        "c",
        "o",
        "h"
      ],
      "commands": [
        {
          "command": ":nohl"
        }
      ]
    },
    {
      "before": [
        "="
      ],
      "commands": [
        {
          "command": "editor.action.formatDocument",
        }
      ]
    }
  ],
  "settings.cycle": [
    {
      "id": "colorTheme",
      "values": [
        {
          "workbench.colorTheme": "Solarized Dark",
        },
        {
          "workbench.colorTheme": "Solarized Light",
        }
      ]
    }
  ],
  "explorer.confirmDelete": false,
  "jest.autoEnable": false,
  "editor.codeActionsOnSave": {},
  "auto-close-tag.fullMode": true,
  "editor.guides.indentation": false,
  "terminal.external.linuxExec": "xterm-256color",
  "terminal.integrated.copyOnSelection": true,
  "workbench.colorTheme": "Solarized Dark",
  "vscode-home-assistant.hostUrl": "http://192.168.1.101:8123",
  "vscode-home-assistant.longLivedAccessToken": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJmMWI4YTJhZDYzNDI0NDEzOGExZjlkMmU2ZTA3MzhlZCIsImlhdCI6MTcwNTYzNDQxMSwiZXhwIjoyMDIwOTk0NDExfQ.5Hc1ZSyWcfGD26NeXNkaEA2WhyfLFYCQDqpjwMy6GYw",
  "settingsSync.ignoredExtensions": [
    "keesschollaart.vscode-home-assistant"
  ],
  "accessibility.signals.terminalBell": {
    "sound": "on"
  },
  "terminal.integrated.enableVisualBell": true,
}