#alias docker.quickstart="/bin/bash --login ~/.dotfiles/docker/start.sh"

## short commands
alias d="docker"
alias dk="docker"
alias dp="docker ps"
alias dr="docker rm"
alias dra='docker rm $(docker ps -a -q)'
alias dm="docker-machine"
alias dl="docker-machine ls"
alias dc="docker-compose"
