# vimrc editing
alias evim='$EDITOR ~/.vim/settings/*'
alias evundles='$EDITOR ~/.vim/vundles/*'
alias ezsh='$EDITOR ~/.zsh/settings/*'
alias ekarabiner='$EDITOR ~/.dotfiles/karabiner/*'
alias eeditor='$EDITOR ~/.editorconfig'
alias ebrew='$EDITOR ~/.dotfiles/Brewfile'
alias egig='$EDITOR ~/.dotfiles/git/ignore'

alias elocal='$EDITOR ~/.local/**/*.zshrc'
alias ebookmark='$EDITOR ~/.local/zsh/bookmarks.zshrc'
